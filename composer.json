{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.2",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/chosen": "^3.0",
        "drupal/colorbox": "^2.0",
        "drupal/commerce": "^2.32",
        "drupal/commerce_quick_edit": "^1.5",
        "drupal/commerce_shipping": "^2.4",
        "drupal/commerce_stock": "^1.0",
        "drupal/context": "^4.1",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/devel_entity_updates": "^4.0",
        "drupal/ds": "^3.15",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_clone": "^2.0@alpha",
        "drupal/facets": "^1.8",
        "drupal/facets_pretty_paths": "1.x-dev@dev",
        "drupal/gin": "3.x-dev@dev",
        "drupal/gin_login": "^1.3",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_slider": "^1.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/module_filter": "^3.2",
        "drupal/owlcarousel": "1.x-dev@dev",
        "drupal/paragraphs": "^1.15",
        "drupal/paragraphs_edit": "^2.0",
        "drupal/pathauto": "^1.11",
        "drupal/redirect": "^1.8",
        "drupal/search_api": "^1.28",
        "drupal/shs": "^2.0@RC",
        "drupal/slick": "^2.6",
        "drupal/slick_paragraphs": "^2.2",
        "drupal/slick_views": "^2.6",
        "drupal/taxonomy_access_fix": "^3.3",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/tb_megamenu": "^1.7",
        "drupal/token": "^1.11",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/vartheme_bs4": "^9.0",
        "drupal/views_bootstrap": "^5.4",
        "drush/drush": "^11.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "kint-php/kint": "^4.2"
    }
}
